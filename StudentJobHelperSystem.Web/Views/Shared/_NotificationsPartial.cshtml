@using static StudentJobHelperSystem.Common.NotificationMessagesConstants;

@if(TempData.ContainsKey(ErrorMessage))
{
    <script>toastr.error('@TempData[ErrorMessage]');</script>
}

@if(TempData.ContainsKey(WarningMessage))
{
    <script>toastr.warning('@TempData[WarningMessage]');</script>
}

@if(TempData.ContainsKey(InformationMessage))
{
    <script>toastr.info('@TempData[InformationMessage]');</script>
}

@if(TempData.ContainsKey(SuccessMessage))
{
    <script>toasrt.success('@TempData[SuccessMessage])';</script>
}
